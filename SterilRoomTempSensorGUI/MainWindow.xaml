<Window x:Class="SterilRoomTempSensorGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SterilRoomTempSensorGUI"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="Temperatur Ruang Sterilisasi" Height="720" Width="1280" WindowStartupLocation="CenterScreen" Icon="Resources/temp-icon.png">
    <Window.DataContext>
        <local:ViewModel/>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Model="{Binding ChartModel}"/>
    </Grid>
</Window>
